{
  "name": "ai-cv-evaluator",
  "version": "1.0.0",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "worker": "node scripts/startWorker.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "docs:serve": "node scripts/docs-server.js",
    "docs:generate": "swagger-jsdoc -d src/config/swagger.js src/routes/v1/*.js -o swagger.json",
    "docs:validate": "swagger-jsdoc -d src/config/swagger.js src/routes/v1/*.js --yaml",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up -d",
    "docker:down": "docker-compose -f docker-compose.dev.yml down",
    "mongodb:start": "docker run -d --name mongodb-cv -p 27017:27017 -e MONGO_INITDB_ROOT_USERNAME=admin -e MONGO_INITDB_ROOT_PASSWORD=admin123 mongo:7.0",
    "mongodb:stop": "docker stop mongodb-cv && docker rm mongodb-cv"
  },
  "type": "module",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "axios": "^1.6.0",
    "bcryptjs": "^3.0.2",
    "bull": "^4.11.5",
    "bullmq": "^5.61.2",
    "chromadb": "^1.8.1",
    "chromadb-default-embed": "^2.14.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.21.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^8.1.0",
    "faker": "^6.6.6",
    "form-data": "^4.0.4",
    "helmet": "^8.1.0",
    "http-status": "^2.1.0",
    "ioredis": "^5.8.2",
    "joi": "^18.0.1",
    "jsonwebtoken": "^9.0.2",
    "mammoth": "^1.6.0",
    "marked": "^16.4.1",
    "moment": "^2.30.1",
    "mongoose": "^8.19.1",
    "morgan": "^1.10.1",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^3.3.2",
    "node-mocks-http": "^1.17.2",
    "openai": "^6.5.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pdf2json": "^3.2.2",
    "pg": "^8.16.3",
    "puppeteer": "^24.25.0",
    "redis": "^4.6.0",
    "supertest": "^7.1.4",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^9.0.1",
    "validator": "^13.15.15",
    "winston": "^3.18.3",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.1.0",
    "@jest/globals": "^30.2.0",
    "jest": "^30.2.0"
  }
}
